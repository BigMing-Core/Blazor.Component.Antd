@inherits AbsDropdown

<lnplacehoder id="@($"main_{IdentityKey}")">
    <a class="@ClassName"
       style="@CStyle"
       onClick="LuanNiaoBlazor.BlockClickEvent()">
        @ChildContent
    </a>
    <div style="position: absolute; top: 0px; left: 0px; width: 100%;" i>
        <div>
            <div class="@_hideDivClassNameStr" style="@_hideSubMenuDivStyleStr" id="@($"hideDiv_{IdentityKey}")">
                @Overlay
            </div>
        </div>
    </div>

</lnplacehoder>
